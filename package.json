{
  "name": "vsparse",
  "version": "1.0.0",
  "description": "Parse key information out of Visual Studio solution (.sln) and project (.csproj, .fsproj, .vbproj) files",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "jest": "^29.4.0",
    "jest-extended": "^4.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^5.3.3"
  },
  "compilerOptions": {
    "module": "commonJs",
    "target": "es6",
    "outDir": "build",
    "lib": [
      "es6"
    ]
  },
  "files": [
    "build/**/*.js",
    "build/**/*.ts"
  ],
  "types": "./build/index.d.ts",
  "exports": {
    ".": "./build/index.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "scripts": {
    "test": "npm run build && npx jest --verbose --silent=false",
    "build": "tsc && npm run codegen",
    "codegen": "npx ts-node scripts/codegen.ts",
    "prepublish": "rm -rf ./build && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lkoskela/vsparse.git"
  },
  "keywords": [
    "visual",
    "studio",
    "vs",
    "dotnet",
    "csproj",
    "fsproj",
    "vbproj",
    "sln",
    "parser"
  ],
  "author": "Lasse Koskela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lkoskela/vsparse/issues"
  },
  "homepage": "https://github.com/lkoskela/vsparse"
}
